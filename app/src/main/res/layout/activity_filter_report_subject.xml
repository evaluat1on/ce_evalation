<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".Filter_report_subject">

    <TextView
            android:text="ปีการศึกษา"
            android:layout_width="141dp"
            android:layout_height="62dp" android:id="@+id/text_year1"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"
            android:layout_marginStart="8dp"
            app:layout_constraintStart_toStartOf="parent"
            android:layout_marginTop="8dp"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintHorizontal_bias="0.03"
            app:layout_constraintVertical_bias="0.121"
            android:textAlignment="viewStart"
            android:textSize="24dp"
            android:textColor="#FA0F0F0F"
            android:textStyle="bold"/>
    <TextView
            android:text="ภาคเรียน"
            android:layout_width="141dp"
            android:layout_height="62dp"
            android:id="@+id/text_term1"
            app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginBottom="8dp"
            app:layout_constraintBottom_toBottomOf="parent"
            android:layout_marginTop="8dp"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="8dp"
            app:layout_constraintHorizontal_bias="0.03"
            app:layout_constraintVertical_bias="0.257"
            android:textAlignment="viewStart"
            android:textSize="24dp"
            android:textColor="#FC0E0D0D"
            android:textStyle="bold"/>
    <TextView
            android:text="ชั้นปี"
            android:layout_width="141dp"
            android:layout_height="62dp"
            android:id="@+id/text_studentyear1"
            app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginEnd="8dp"
            app:layout_constraintBottom_toBottomOf="parent"
            android:layout_marginTop="8dp"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="8dp"
            app:layout_constraintHorizontal_bias="0.031"
            app:layout_constraintVertical_bias="0.399"
            android:textAlignment="viewStart"
            android:textSize="24dp"
            android:textColor="#FA0E0D0D"
            android:textStyle="bold"/>
    <TextView
            android:text="หลักสูตร"
            android:layout_width="141dp"
            android:layout_height="62dp"
            android:id="@+id/text_curri1"
            app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginEnd="8dp"
            app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="8dp"
            app:layout_constraintBottom_toBottomOf="parent"
            android:layout_marginTop="8dp"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintHorizontal_bias="0.031"
            app:layout_constraintVertical_bias="0.544"
            android:textAlignment="viewStart"
            android:textSize="24dp"
            android:textColor="#F70A0A0A"
            android:textStyle="bold"/>
    <Spinner
            android:layout_width="141dp"
            android:layout_height="62dp"
            android:id="@+id/spinneryear1"
            app:layout_constraintStart_toEndOf="@+id/text_year1"
            app:layout_constraintBottom_toBottomOf="@+id/text_year1"
            app:layout_constraintTop_toTopOf="@+id/text_year1"
            app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginEnd="8dp"
            app:layout_constraintHorizontal_bias="0.923"
            app:layout_constraintVertical_bias="0.0"/>
    <Spinner
            android:layout_width="141dp"
            android:layout_height="62dp"
            android:id="@+id/spinnerterm1"
            app:layout_constraintTop_toTopOf="@+id/text_term1"
            app:layout_constraintStart_toEndOf="@+id/text_term1"
            app:layout_constraintVertical_bias="0.0"
            app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginEnd="8dp"
            app:layout_constraintHorizontal_bias="0.923"
            app:layout_constraintBottom_toBottomOf="@+id/text_term1"/>
    <Spinner
            android:layout_width="141dp"
            android:layout_height="62dp"
            android:id="@+id/spinnerstudentyear1"
            app:layout_constraintStart_toEndOf="@+id/text_studentyear1"
            app:layout_constraintBottom_toBottomOf="@+id/text_studentyear1"
            app:layout_constraintTop_toTopOf="@+id/text_studentyear1"
            app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginEnd="8dp"
            app:layout_constraintHorizontal_bias="0.923"
            app:layout_constraintVertical_bias="1.0"/>
    <Spinner
            android:layout_width="141dp"
            android:layout_height="62dp"
            android:id="@+id/spinnercurri1"
            app:layout_constraintBottom_toBottomOf="@+id/text_curri1"
            app:layout_constraintTop_toTopOf="@+id/text_curri1"
            app:layout_constraintStart_toEndOf="@+id/text_curri1"
            app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginEnd="8dp"
            app:layout_constraintHorizontal_bias="0.923"
            app:layout_constraintVertical_bias="1.0"
            android:layout_marginStart="8dp"/>

</android.support.constraint.ConstraintLayout>