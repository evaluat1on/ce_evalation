<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".Filter_subject">

    <LinearLayout
            android:orientation="vertical"
            android:layout_width="0dp"
            android:layout_height="0dp" android:id="@+id/linearLayout" app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toTopOf="parent" app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintBottom_toBottomOf="parent" app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintVertical_bias="1.0">
    </LinearLayout>
    <TextView
            android:layout_width="142dp"
            android:layout_height="45dp"
            android:text="ปีการศึกษา"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintTop_toTopOf="parent" app:layout_constraintHorizontal_bias="0.047"
            app:layout_constraintVertical_bias="0.053" android:id="@+id/textView10" android:textSize="24sp"/>
    <TextView
            android:layout_width="142dp"
            android:layout_height="47dp"
            android:text="ภาคเรียน"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintHorizontal_bias="0.045"
            app:layout_constraintVertical_bias="0.061" android:id="@+id/textView13" android:layout_marginTop="8dp"
            app:layout_constraintTop_toBottomOf="@+id/textView10" android:textSize="24sp"/>
    <TextView
            android:layout_width="142dp"
            android:layout_height="49dp"
            android:text="ชั้นปี"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintHorizontal_bias="0.045"
            app:layout_constraintVertical_bias="0.079" android:id="@+id/textView14" android:textAlignment="viewStart"
            android:layout_marginTop="8dp" app:layout_constraintTop_toBottomOf="@+id/textView13"
            android:textSize="24sp"/>
    <TextView
            android:layout_width="142dp"
            android:layout_height="41dp"
            android:text="หลักสูตร"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintHorizontal_bias="0.045"
            app:layout_constraintVertical_bias="0.086" android:id="@+id/textView15" android:textAlignment="viewStart"
            android:layout_marginTop="8dp" app:layout_constraintTop_toBottomOf="@+id/textView14"
            android:textSize="24sp"/>
    <Spinner
            android:layout_width="86dp"
            android:layout_height="47dp"
            android:verticalScrollbarPosition="defaultPosition"
            android:id="@+id/spinnerYear"
            app:layout_constraintTop_toTopOf="@+id/textView14"
            app:layout_constraintStart_toEndOf="@+id/textView14"
            android:layout_marginStart="36dp" app:layout_constraintBottom_toBottomOf="@+id/textView14"
            app:layout_constraintEnd_toStartOf="@+id/textView14" android:layout_marginEnd="8dp"
            app:layout_constraintHorizontal_bias="0.0" app:layout_constraintVertical_bias="1.0"/>
    <Spinner
            android:layout_width="87dp"
            android:layout_height="49dp"
            android:verticalScrollbarPosition="defaultPosition"
            android:id="@+id/spinnerTerm"
            app:layout_constraintTop_toTopOf="@+id/textView15"
            app:layout_constraintBottom_toBottomOf="@+id/textView15"
            app:layout_constraintStart_toEndOf="@+id/textView15" android:layout_marginStart="36dp"
            app:layout_constraintVertical_bias="1.0" app:layout_constraintEnd_toStartOf="@+id/textView15"
            android:layout_marginEnd="8dp" app:layout_constraintHorizontal_bias="0.0"/>
    <Spinner
            android:layout_width="85dp"
            android:layout_height="44dp"
            android:verticalScrollbarPosition="defaultPosition"
            android:id="@+id/spinnerLevel"
            app:layout_constraintTop_toTopOf="@+id/textView13"
            app:layout_constraintStart_toEndOf="@+id/textView13"
            android:layout_marginStart="36dp" app:layout_constraintBottom_toBottomOf="@+id/textView13"
            app:layout_constraintEnd_toStartOf="@+id/textView13" android:layout_marginEnd="8dp"
            app:layout_constraintHorizontal_bias="0.0" app:layout_constraintVertical_bias="1.0"/>
    <Spinner
            android:layout_width="85dp"
            android:layout_height="43dp"
            android:verticalScrollbarPosition="defaultPosition"
            android:id="@+id/spinnerCuri" app:layout_constraintStart_toEndOf="@+id/textView10"
            android:layout_marginStart="36dp"
            app:layout_constraintTop_toTopOf="@+id/textView10"
            app:layout_constraintBottom_toBottomOf="@+id/textView10"
            app:layout_constraintEnd_toStartOf="@+id/textView10" android:layout_marginEnd="8dp"
            app:layout_constraintHorizontal_bias="0.0" app:layout_constraintVertical_bias="1.0"/>


</android.support.constraint.ConstraintLayout>